{
    "default": "simulated",
    "byInstrumentType": {
        "CX": "simulated",
        "EQ": "dwx",
        "FX": "dwx",
        "CFD": "dwx"
    },
    "providers": {
        "j2t": {
            "baseURL": "https://api.j2t.com",
            "accountId": "${ENV:J2T_ACCOUNT_ID}",
            "accessToken": "${ENV:J2T_TOKEN}",
            "timeoutMs": 10000,
            "defaultDuration": "gtc"
        },
        "dwx": {
            "metatraderDirPath" : "${ENV:DWX_SOCKET_DIR}",
            "openOrderRetryMaxDelayMs" : "${ENV:DWX_MAX_RETRY_DELAY_MS}"
        }
    }
}
