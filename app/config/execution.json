{
  "default": "simulated",
  "byInstrumentType": {
    "CX": "simulated",
    "EQ": "j2t"
  },
  "providers": {
    "j2t": {
      "baseURL": "https://api.j2t.com",
      "accountId": "${ENV:J2T_ACCOUNT_ID}",
      "accessToken": "${ENV:J2T_TOKEN}",      
      "timeoutMs": 10000,
      "defaultDuration": "gtc"
    }
  }
}
